<script type="text/javascript" language="javascript">

  document.addEventListener("DOMContentLoaded", function(event) {
    var basicUrl = document.forms['signup-form'].action;
    var passingUrl = basicUrl.concat(location.search);
    document.forms['signup-form'].action = passingUrl;
    
    if (window.name) {
      var data = JSON.parse(window.name);
      
      $('#category_1').change();
      $('#position_1').val(data.position_1);
      $('#experience_1').val(data.experience_1);
      
      $('#category_2').change();
      $('#position_2').val(data.position_2);
