  $('#category_1').change(function() {
    $('#position_1').removeAttr('disabled');
    $('#position_1').empty();
    $('#position_1').append("<option value=''>- Selecione -</option>");
    $('#experience_1').removeAttr('disabled');
    $('#experience_1').val("");
    
    if ($('#category_1').val() == "") {
      $('#position_1').attr('disabled', 'disabled');
      $('#experience_1').attr('disabled', 'disabled');
    } else {
      $.each(jobs, function(index, item) {
        var selectedCategory = $('#category_1').val();
      
        if (selectedCategory == item.category) {
          $("#position_1").append("<option value='"+item.value+"'>"+item.name+"</option>");
        }
      });
    }
  });
  
  $('#category_2').change(function() {
    $('#position_2').removeAttr('disabled');
    $('#position_2').empty();
    $('#position_2').append("<option value=''>- Selecione -</option>");
    $('#experience_2').removeAttr('disabled');
    $('#experience_2').val("");
    
    if ($('#category_2').val() == "") {
      $('#position_2').attr('disabled', 'disabled');
      $('#experience_2').attr('disabled', 'disabled');
    } else {
      $.each(jobs, function(index, item) {
        var selectedCategory = $('#category_2').val();
      
        if (selectedCategory == item.category) {
          $("#position_2").append("<option value='"+item.value+"'>"+item.name+"</option>");
        }
      });
    }
  });
  
  $('#category_3').change(function() {
    $('#position_3').removeAttr('disabled');
    $('#position_3').empty();
    $('#position_3').append("<option value=''>- Selecione -</option>");
    $('#experience_3').removeAttr('disabled');
    $('#experience_3').val("");
    
    if ($('#category_3').val() == "") {
      $('#position_3').attr('disabled', 'disabled');
      $('#experience_3').attr('disabled', 'disabled');
    } else {
      $.each(jobs, function(index, item) {
        var selectedCategory = $('#category_3').val();
      
        if (selectedCategory == item.category) {
          $("#position_3").append("<option value='"+item.value+"'>"+item.name+"</option>");
        }
      });
    }
  });

  $('#signup-form').submit(function() {
    var data = {
      "position_1": $('#position_1').val(), "experience_1": $('#experience_1').val(),
      "position_2": $('#position_2').val(), "experience_2": $('#experience_2').val(),
      "position_3": $('#position_3').val(), "experience_3": $('#experience_3').val(),
    };
    window.name = JSON.stringify(data);
  });

</script>
